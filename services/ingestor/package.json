{
  "name": "ingestor",
  "version": "1.0.0",
  "description": "Gig data ingestion service with plugin architecture",
  "main": "dist/index.js",
  "type": "module",
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "typecheck": "tsc --noEmit",
    "ingest:all": "tsx src/cli.ts ingest:all",
    "ingest:source": "tsx src/cli.ts ingest:source",
    "validate": "tsx src/cli.ts validate",
    "merge": "tsx src/cli.ts merge",
    "daemon": "tsx src/cli.ts daemon",
    "stats": "tsx src/cli.ts stats",
    "stats:detailed": "tsx src/cli.ts stats --detailed",
    "scheduler:status": "tsx src/cli.ts scheduler:status",
    "scheduler:stop": "tsx src/cli.ts scheduler:stop",
    "scheduler:list": "tsx src/cli.ts scheduler:list",
    "scheduler:trigger": "tsx src/cli.ts scheduler:trigger",
    "scheduler:health": "tsx src/cli.ts scheduler:health",
    "scheduler:performance": "tsx src/cli.ts scheduler:performance",
    "config:show": "tsx src/cli.ts config:show",
    "config:validate": "tsx src/cli.ts config:validate",
    "config:init": "tsx src/cli.ts config:init",
    "test": "jest",
    "test:watch": "jest --watch"
  },
  "dependencies": {
    "@gigateer/contracts": "workspace:*",
    "bottleneck": "^2.19.5",
    "commander": "^12.0.0",
    "dotenv": "^17.2.1",
    "mongodb": "^6.18.0",
    "node-cron": "^3.0.3",
    "pino": "^8.19.0",
    "pino-pretty": "^10.3.0",
    "playwright": "^1.41.0",
    "zod": "^3.22.0"
  },
  "devDependencies": {
    "@types/jest": "^30.0.0",
    "@types/mongodb": "^4.0.7",
    "@types/node": "^20.0.0",
    "@types/node-cron": "^3.0.11",
    "jest": "^29.7.0",
    "ts-jest": "^29.4.1",
    "tsx": "^4.7.0",
    "typescript": "^5.4.0"
  }
}